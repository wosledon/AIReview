{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AIReview.db",
    "RedisConnection": "localhost:6379,password=your_redis_password_here,abortConnect=false,connectTimeout=5000,syncTimeout=5000"
  },
  "Redis": {
    "Configuration": "localhost:6379",
    "InstanceName": "AIReview:",
    "DefaultExpiry": "01:00:00",
    "LockTimeout": "00:30:00",
    "EnableLogging": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information",
      "StackExchange.Redis": "Warning"
    }
  },
  "AllowedHosts": "*"
}
